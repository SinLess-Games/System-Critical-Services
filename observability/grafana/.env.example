# observability/grafana/.env.example

# Grafana security credentials
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD="your_grafana_admin_password"

# MySQL root password used by Grafana for its database
MYSQL_ROOT_PASSWORD="your_mysql_root_password"

# Secret key for Grafana (used for signing, session encryption, etc.)
GRAFANA_SECRET_KEY="your_grafana_secret_key"

# Secret for Prometheus exporter integration
PROMETHEUS_EXPORTER_SECRET="your_prometheus_exporter_secret"

# RabbitMQ password (if used for alerting or other integrations)
RABBITMQ_PASSWORD="your_rabbitmq_password"

# InfluxDB credentials for Grafana data source
INFLUX_USER="admin"
INFLUX_PASSWORD="your_influxdb_password"
# InfluxDB API token for authentication
INFLUX_TOKEN="your_influx_token"

# Discord webhook URL for alert notifications
DISCORD_WEBHOOK="your_discord_webhook_url"

# GitHub Personal Access Token for integration (adjust scopes as needed)
GITHUB_ACCESS_TOKEN="your_github_access_token"
